<!DOCTYPE html>
<html>
  <head>
    <title>Cs2</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <%= link_to "Home", :root %>
    <% if @dataset.present? %>
      >&nbsp;<%= link_to "Dataset", dataset_path( @dataset ) %>
    <% elsif @source.present? %>
      >&nbsp;<%= link_to "Dataset", dataset_path( @source.dataset ) %>
      >&nbsp;<%= link_to "Source", source_path( @source ) %>
    <% end %><br>
    <p class="notice"><%= notice %></p>
    <p class="alert"><%= alert %></p>
    <%= yield %>
  </head>

  <body>
  </body>
    <%= link_to "Home", :root %>
    <% if @dataset.present? %>
      >&nbsp;<%= link_to "Dataset", dataset_path( @dataset ) %>
    <% elsif @source.present? %>
      >&nbsp;<%= link_to "Dataset", dataset_path( @source.dataset ) %>
      >&nbsp;<%= link_to "Source", source_path( @source ) %>
    <% end %><br>
  <% if user_signed_in? %>
    </br><%= link_to('Logout', destroy_user_session_path, method: :delete) %>        
  <% end %>
</html>
